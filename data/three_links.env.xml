<Environment>
  <Robot name="3DOFRobot" file="three_links.robot.xml">
    <translation>0 0 0</translation>
  </Robot>
  <KinBody name="KinBodyObstacle">
    <Body name="BodyObstacle" type="static">
      <Geom type="box">
        <Extents>0.05 0.05 0.3</Extents>
        <!-- set the translation and rotation of the box. Note that the transforamtions-->
        <!-- are all relative to the parent body.-->
        <Translation>0.2 0.25 0</Translation>
      </Geom>
    </Body>
  </KinBody>
</Environment>
