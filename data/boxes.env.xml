<Environment>
  
  <Robot name="PointRobot">
  <KinBody>
    <Body name="Base" type="dynamic">
      <Translation>-0.3 0.9 0.0</Translation>
      <rotationaxis>1 0.5 0 30</rotationaxis>
      <Geom type="box">
        <Extents>0.12 0.06 0.03</Extents>
        <diffuseColor>0 0.8 0</diffuseColor>
      </Geom>
    </Body>
  </KinBody>
  
  <Manipulator name="base">
    <effector>Base</effector>
    <base>Base</base>
    <Translation>0 0 0</Translation>
  </Manipulator>
</Robot>
  
  <Robot name="ObstacleRobot">
		<KinBody>
		  <Body name="Obstacle" type="dynamic">
		    <Translation> 0.2 0.75 0.1 </Translation>
		    <rotationaxis>1 0 0 90</rotationaxis>
		    <Geom type="box">
		      <extents> 0.05 0.07 0.03 </extents>
		      <diffuseColor>0.8 0 0</diffuseColor>
		    </Geom>
		  </Body>
		</KinBody>
		
		<Manipulator name="obstacle">
		  <effector>Obstacle</effector>
		  <base>Obstacle</base>
		  <Translation>0 0 0</Translation>
		</Manipulator>
	</Robot>
  
</Environment>



