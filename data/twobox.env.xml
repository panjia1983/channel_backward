<?xml version="1.0" encoding="ASCII"?>
<Environment>
  <KinBody name="box1">
    <Body name="box" type="dynamic">
      <Translation>0 1 6</Translation>
      <!--rotation of the body specified as a rotation axis. First three numbers are the axis.-->
      <Mass type="mimicgeom">
        <density>1</density>
      </Mass>
      
      <Geom type="box">
        <Extents>4 1.5 0.2</Extents>
      </Geom>
    </Body>
  </KinBody>
  <KinBody name="box2">
    <Body name="box" type="dynamic">
      <Translation>0 -1 3</Translation>
      <!--rotation of the body specified as a rotation axis. First three numbers are the axis.-->
      <Mass type="mimicgeom">
        <density>1</density>
      </Mass>
      
      <Geom type="box">
        <Extents>4 1.5 0.2</Extents>
      </Geom>
    </Body>
  </KinBody>

</Environment>